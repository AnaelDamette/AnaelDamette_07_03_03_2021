},\r\n  methods: {\r\n    getPost() {\r\n      axios\r\n        .get(\"http://localhost:3000/api/post\")\r\n        .then((response) => {\r\n          console.log(\"post\", response.data);\r\n          this.allPosts = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    reload(reload){\r\n      this.getPost()\r\n      console.log(reload)\r\n    },\r\n    deletePost(deleteUuidPost) {\r\n      console.log(\"test : \" + deleteUuidPost);\r\n      let uuid = localStorage.getItem(\"uuid\");\r\n      console.log(\"le post uuid : \" + deleteUuidPost + \" l'user uuid\" + uuid);\r\n      axios\r\n        .delete(\"http://localhost:3000/api/post/delete/\" + uuid, {\r\n          data: {\r\n            uuidPost: deleteUuidPost,\r\n            userIdOrder: uuid,\r\n          },\r\n        })\r\n        .then(() => {\r\n          this.getPost();\r\n        })\r\n        .catch((error) => console.log(error));\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getPost();\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}