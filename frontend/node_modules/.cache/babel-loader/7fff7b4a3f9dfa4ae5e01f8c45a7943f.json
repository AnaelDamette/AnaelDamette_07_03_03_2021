UAAA,UAAA,GAAA,8IAAA;AACA,UAAA,aAAA,GAAA,uCAAA;;AAEA,UACA,KAAA,UAAA,CAAA,KAAA,IACA,KAAA,UAAA,CAAA,QADA,IAEA,KAAA,UAAA,CAAA,QAFA,IAGA,aAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,QAAA,CAHA,IAIA,UAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,KAAA,CAJA,IAKA,aAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,QAAA,CANA,EAOA;AACA,QAAA,KAAA,CACA,IADA,CACA,GADA,EACA,KAAA,UADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA,SALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,cAAA,KAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,6BAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA,GAAA,GAAA,GAAA,mBAAA,GAAA,KAAA;AACA;AACA,SAZA;AAaA,OArBA,MAqBA;AACA,QAAA,KAAA,CAAA,gDAAA,CAAA;AACA;AACA;AAxCA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <form class=\"row d-flex jumbotron shadow-box opacity ml-auto mr-auto mt-5\">\r\n      <div class=\"col-sm-6 p-3 d-flex justify-content-center\">\r\n        <figure><img src=\"../img/icon-above-font.png\" class=\"w-75\" /></figure>\r\n      </div>\r\n      <div class=\"col-sm-6 p-3 d-flex flex-column justify-content-center\">\r\n        <div class=\"form-group\">\r\n          <label for=\"inputEmail\"><p class=\"m-0\">Email Groupomania</p></label>\r\n          <input\r\n            type=\"email\"\r\n            class=\"form-control\"\r\n            id=\"inputEmail\"\r\n            v-model=\"dataSignup.email\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"inputUsername\"><p class=\"m-0\">username</p></label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"inputUsername\"\r\n            v-model=\"dataSignup.username\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"inputPassword\"><p class=\"m-0\">Mot de passe</p></label>\r\n          <input\r\n            type=\"password\"\r\n            class=\"form-control\"\r\n            id=\"inputPassword\"\r\n            v-model=\"dataSignup.password\"\r\n          />\r\n        </div>\r\n        <div class=\"d-flex justify-content-end\">\r\n          <button\r\n            @click.prevent=\"sendSignup\"\r\n            type=\"submit\"\r\n            class=\"btn btn-primary w-25\"\r\n          >\r\n            Submit\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nimport axios from \"axios\";\r\nimport router from \"../router/index\";\r\nexport default {\r\n  name: \"SignUp\",\r\n  data() {\r\n    return {\r\n      dataSignup: {\r\n        username: null,\r\n        email: null,\r\n        password: null,\r\n        isAdmin: false,\r\n      },\r\n      msg: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState([\"User\"]),\r\n  },\r\n  methods: {\r\n    sendSignup() {\r\n      console.log(\r\n        \"test N°1 je suis dans sendSignup \" +\r\n          this.dataSignup.email +\r\n          \" \" +\r\n          this.dataSignup.username +\r\n          \" \" +\r\n          this.dataSignup.password\r\n      );\r\n      const url = \"http://localhost:3000/api/auth/signup\";\r\n      const regexPassword = /\\D/;\r\n      //const regexPassword = /((?=.*\\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[\\W]).{6,64})/; regex Password avec 1 charactère spécial 1 majuscule minimum\r\n      const regexEmail = /^[a-z0-9!#$ %& '*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&' * +/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?$/g;\r\n      const usernameRegex = /^(?!.*\\.\\.)(?!.*\\.$)[^\\W][\\w.]{0,29}$/;\r\n\r\n      if (\r\n        (this.dataSignup.email) &&\r\n        (this.dataSignup.username) &&\r\n        (this.dataSignup.password) &&\r\n        regexPassword.test(this.dataSignup.password) &&\r\n        regexEmail.test(this.dataSignup.email) &&\r\n        usernameRegex.test(this.dataSignup.username)\r\n      ) {\r\n        axios\r\n          .post(url, this.dataSignup)\r\n          .then((response) => {\r\n            console.log(response);\r\n            router.push(\"/Login\");\r\n          })\r\n          .catch((error) => {\r\n            if (error.response.status == 409) {\r\n              alert(\"L'utilisateur existe déjà !\");\r\n            } else {\r\n              console.log(error.status + \" \" + \"voici l'erreur : \" + error);\r\n            }\r\n          });\r\n      } else {\r\n        alert(\"Un problème, lors du renseignement du document\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}